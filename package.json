{
  "name": "graphql-api-koa",
  "version": "2.2.0",
  "description": "Koa middleware for GraphQL execution and error handling, written from scratch for Koa.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/graphql-api-koa",
  "homepage": "https://github.com/jaydenseric/graphql-api-koa#readme",
  "bugs": "https://github.com/jaydenseric/graphql-api-koa/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "graphql",
    "api",
    "koa"
  ],
  "files": [
    "lib",
    "!lib/test"
  ],
  "main": "lib",
  "module": "lib/index.mjs",
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "graphql": "0.13.1 - 14"
  },
  "dependencies": {
    "http-errors": "^1.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-require-extensions": "^2.0.0",
    "coverage-node": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-env": "^12.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^18.4.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql": "^14.4.2",
    "jsdoc-md": "^4.0.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "test-director": "^3.0.0"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:mjs && npm run prepare:js && npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:clean": "rm -rf lib",
    "prepare:mjs": "BABEL_ESM=1 babel src -d lib --keep-file-extension",
    "prepare:js": "babel src -d lib",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier 'lib/**/*.{mjs,js}' readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:eslint": "eslint . --ext mjs,js",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:api": "coverage-node lib/test",
    "prepublishOnly": "npm test"
  }
}
